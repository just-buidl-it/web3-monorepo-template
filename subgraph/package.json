{
  "name": "subgraph",
  "version": "0.1.0",
  "scripts": {
    "create": "graph create  --node https://api.thegraph.com/deploy/",
    "create:local": "graph create  --node http://127.0.0.1:8020 subgraph",
    "codegen": "graph codegen",
    "generate-subgraph-types": "rm -rf /graph-client/.graphclient && yarn graphclient build --dir ./graph-client",
    "build": "graph build",
    "deploy": "graph deploy --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy:local": "graph deploy --ipfs http://127.0.0.1:5001 --node http://127.0.0.1:8020 subgraph",
    "write-config": "sh -c 'mustache config/${0}.json template.yaml > subgraph.yaml'",
    "test": "graph test",
    "test:integration": "hardhat test tests/integration/index.ts --network localhost",
    "remove:local": "npx graph remove --node http://127.0.0.1:8020/ subgraph",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "dependencies": {
    "@graphprotocol/graph-ts": "^0.27.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "protocol": "link:../protocol"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.4.7",
    "@ethersproject/providers": "^5.4.7",
    "@graphprotocol/client-cli": "^2.2.16",
    "@graphprotocol/graph-cli": "^0.30.2",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": ">=12.0.0",
    "apollo-fetch": "^0.7.0",
    "chai": "^4.2.0",
    "ethers": "^5.4.7",
    "hardhat": "^2.12.4",
    "hardhat-deploy": "^0.11.22",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.8",
    "matchstick-as": "^0.5.0",
    "mustache": "^4.2.0",
    "solidity-coverage": "^0.8.0",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.9.4",
    "urql": "^3.0.3"
  },
  "_moduleAliases": {
    "@nomiclabs/hardhat-ethers": "node_modules/hardhat-deploy-ethers",
    "@nomiclabs/hardhat-ethers/types": "hardhat-deploy-ethers/types"
  }
}
